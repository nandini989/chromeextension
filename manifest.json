{
  "manifest_version": 2,

  "name": "To Do List plugin",
  "minimum_chrome_version": "72",
  "description": "This extension will make a To DO list.",
  "version": "1.0",
  
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "storage"
   ]
   
}